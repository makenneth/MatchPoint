{
  "name": "matchPoint",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build . --ignore node_modules,test",
    "build": "better-npm-run build",
    "prestart": "npm run build",
    "start": "npm start-dev",
    "create-tables": "bash ./bin/prepareDatabase.sh",
    "pretest-node": "npm run create-tables && DB_USER=test DB=matchpoints_test babel-node ./app/test/dbSetup.js",
    "start-dev": "better-npm-run start-dev",
    "test-node": "DB_USER=test DB=matchpoints_test mocha -R spec --compilers js:babel-core/register app/test/model/test-club.js"
  },
  "betterScripts": {
    "start-dev": {
      "command": "nodemon ./app/app.js --exec babel-node",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "sudo node ./build/app.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 80
      }
    },
    "build": {
      "command": "npm run clean && npm run build-server"
    }
  },
