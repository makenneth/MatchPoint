{
  "passPerPreset": true,
  "presets": [
    { "plugins": [ "transform-runtime" ] },
    {
      "passPerPreset": false,
      "presets": ["react", "es2015", "stage-0"],
    }
  ],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-async-to-generator"
  ],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  }
}
